### ALIASES ###

# list commands
alias ls='ls -ltaX'
alias la='ls -a'
alias ll='ls -alFh'
alias l='ls'
alias l.="ls -A | egrep '^\.'"

# fix obvious typo's
alias cd..='cd ..'
alias pdw='pwd'
alias udpate='update'

# git commands
alias gits='git status'

# maven commands
alias mvnt='mvn clean test'
alias mvnv='mvn clean verify'
alias mvni='mvn clean install'